<p id="notice"><%= notice %></p>

<h1>Media</h1>

<table>
  <thead>
    <tr>
      <th>Original Path</th>
      <th>Original Filename</th>
      <th>Filename Hash</th>
      <th>Mediatype</th>
      <th>Datetime</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Sequence</th>
      <th>Frame</th>
      <th>Tubecam Device ID</th>
      <th>Exifdata</th>
      <th>Deleted</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @media.each do |medium| %>
      <tr>
        <td><%= medium.original_path %></td>
        <td><%= medium.original_filename %></td>
        <td><%= medium.filename_hash%></td>
        <td><%= medium.mediatype %></td>
        <td><%= medium.datetime %></td>
        <td><%= medium.longitude %></td>
        <td><%= medium.latitude %></td>
        <td><%= medium.sequence %></td>
        <td><%= medium.frame %></td>
        <td><%= medium.tubecam_device_id %></td>
        <td><%= medium.exifdata %></td>
        <td><%= medium.deleted %></td>
        <td><%= link_to 'Show', medium %></td>
        <td><%= link_to 'Edit', edit_medium_path(medium) %></td>
        <td><%= link_to 'Destroy', medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medium', new_medium_path %>
